<?xml version="1.0" encoding="utf-8" ?><ContentPage        xmlns="http://xamarin.com/schemas/2014/forms"        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"        x:Class="exjobb.EditBookingPage">        <ContentPage.Content>        <StackLayout VerticalOptions="StartAndExpand">                        <StackLayout Orientation="Vertical">                            <StackLayout Orientation="Horizontal">                    <Label Text="Dator lista: " TextColor="Black" />            <StackLayout HorizontalOptions="StartAndExpand" x:Name="EditLayout">                <Frame BorderColor="Gray"       CornerRadius="5"       Padding="0">                <StackLayout HeightRequest="200" Spacing="0">                    <Frame BackgroundColor="Orange" MinimumHeightRequest="100" Padding="0">                        <Grid Padding="0" >                            <Grid.ColumnDefinitions>                                <ColumnDefinition Width="2*"/>                                <ColumnDefinition Width="2*"/>                                <ColumnDefinition Width="2*"/>                                <ColumnDefinition Width="2*"/>                                <ColumnDefinition Width="2*"/>                            </Grid.ColumnDefinitions>                            <Label Margin="10,0,10,0" TextColor="Black" Text="Dator ID" VerticalOptions="Center"  Grid.Column="0"/>                            <Label Margin="10,0,10,0" TextColor="Black" Text="Modell" VerticalOptions="Center"  Grid.Column="1" />                            <Label Margin="10,0,10,0" TextColor="Black" Text="Märke" VerticalOptions="Center"  Grid.Column="2" />                            <Label Margin="10,0,10,0" TextColor="Black" Text="Pris/dag(kr)" VerticalOptions="Center"  Grid.Column="3" />                            <Label Margin="10,0,10,0" TextColor="Black" Text="typ" VerticalOptions="Center"  Grid.Column="4" />                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="0" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="1" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="2" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="3" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="4" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                        </Grid>                    </Frame>                    <ListView x:Name="datorList">                        <ListView.ItemTemplate>                            <DataTemplate>                                <ViewCell>                                    <Frame  Padding="0" BorderColor="DarkSlateGray">                                        <Grid Padding="0" RowSpacing="0">                                            <Grid.ColumnDefinitions>                                                <ColumnDefinition Width="2*"/>                                                <ColumnDefinition Width="2*"/>                                                <ColumnDefinition Width="2*"/>                                                <ColumnDefinition Width="2*"/>                                                <ColumnDefinition Width="2*"/>                                            </Grid.ColumnDefinitions>                                            <Label Margin="10,0,10,0" Text="{Binding datorId }" VerticalOptions="Center"  Grid.Column="0"  />                                            <Label Margin="10,0,10,0" Text="{Binding modell }" VerticalOptions="Center"  Grid.Column="1" />                                            <Label Margin="10,0,10,0" Text="{Binding märke }" VerticalOptions="Center"  Grid.Column="2" />                                            <Label Margin="10,0,10,0" Text="{Binding prisPerDag}" VerticalOptions="Center"  Grid.Column="3" />                                            <Label Margin="10,0,10,0" Text="{Binding typ}" VerticalOptions="Center"  Grid.Column="4" />                                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="0" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand" />                                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="1" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand"/>                                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="2" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand"/>                                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="3" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand"/>                                            <BoxView BackgroundColor="DarkSlateGray" WidthRequest="1" Grid.Column="4" HorizontalOptions="EndAndExpand" VerticalOptions="FillAndExpand"/>                                        </Grid>                                    </Frame>                                </ViewCell>                            </DataTemplate>                        </ListView.ItemTemplate>                    </ListView>                </StackLayout>                    </Frame>                </StackLayout>                </StackLayout>                <StackLayout x:Name="editDataLayout"  Orientation="Horizontal">                    <StackLayout Orientation="Vertical" HorizontalOptions="Start">                    <StackLayout Orientation="Vertical" HorizontalOptions="Start">                    <StackLayout Orientation="Vertical">                    <Label Text="Ta bort dator:" VerticalOptions="Center" />                        <Picker x:Name="CategoryPicker" Title="Välj dator" ItemDisplayBinding="{Binding datorId}" SelectedIndexChanged="CategoryPicker_SelectedIndexChanged" WidthRequest="150" HeightRequest="50" />                        </StackLayout>                        <Button Text="Ta bort" VerticalOptions="Center" Clicked="Button_Remove_Computer" />                    </StackLayout>                    <Label Text="Lägg till dator:" />                        <Button Text="Sök"  Clicked="Button_Search_Computers" />                    </StackLayout>                    <StackLayout HorizontalOptions="EndAndExpand" Padding="10" Orientation="Vertical">                                             <StackLayout HorizontalOptions="EndAndExpand" x:Name="searchNewComputersLayout" IsVisible="false" Spacing="0">                            <Label Text="Dator lista:" TextColor="Black" HorizontalOptions="StartAndExpand" VerticalOptions="CenterAndExpand"/>                               <Frame BorderColor="Gray"       Padding="0" >                            <ListView x:Name="datorListSearch" SeparatorColor="Orange" HorizontalOptions="EndAndExpand" SeparatorVisibility="None" SelectionMode="None"  HasUnevenRows="True">                                <ListView.ItemTemplate>                                    <DataTemplate>                                        <ViewCell>                                            <Grid>                                                                             <Frame BorderColor="Gray"                                           Padding="8">                                                <StackLayout Orientation="Horizontal">                                                <StackLayout Orientation="Vertical" VerticalOptions="Center">                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="ID: "   />                                                   <Label VerticalOptions="Center" Text="{Binding datorId}"   />                                                    </StackLayout>                                                    <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Modell: "   />                                                    <Label VerticalOptions="Center" Text="{Binding modell}"  />                                                        </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Type: "   />                                                    <Label VerticalOptions="Center" Text="{Binding typ}"  />                                                    </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Märke: "   />                                                    <Label VerticalOptions="Center" Text="{Binding prisPerDag}"  />                                                    </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Pris/Dag(kr): "   />                                                    <Label VerticalOptions="Center" Text="{Binding prisPerDag}" />                                                    </StackLayout>                                                    </StackLayout>                                                    <Image HorizontalOptions="EndAndExpand" Source="AddImage.png"                                                           WidthRequest="50" HeightRequest="50">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer                                                                Tapped="datorListSearch_ItemTapped"                                                                CommandParameter="{Binding datorId}">                                                                                                                            </TapGestureRecognizer>                                                        </Image.GestureRecognizers>                                                    </Image>                                                        </StackLayout>                                                    </Frame>                                            </Grid>                                        </ViewCell>                                    </DataTemplate>                                </ListView.ItemTemplate>                            </ListView>                                   </Frame>                            <Button Text="Varukorg" HorizontalOptions="EndAndExpand" Clicked="Button_Show_Selected"></Button>                        </StackLayout>                         <StackLayout x:Name="showSelectedLayout" VerticalOptions="StartAndExpand" IsVisible="false">                             <Frame BorderColor="Gray"       Padding="0">                <ListView x:Name="datorListSelected" SeparatorColor="Blue" SeparatorVisibility="None" SelectionMode="None" HasUnevenRows="True">                                <ListView.ItemTemplate>                                    <DataTemplate>                                        <ViewCell>                                            <Grid>                                                                              <Frame BorderColor="Gray"                                           Padding="8">                                                <StackLayout Orientation="Horizontal">                                                 <StackLayout Orientation="Vertical" VerticalOptions="Center">                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="ID: "   />                                                   <Label VerticalOptions="Center" Text="{Binding datorId}"   />                                                    </StackLayout>                                                    <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Modell: "   />                                                    <Label VerticalOptions="Center" Text="{Binding modell}"  />                                                        </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Type: "   />                                                    <Label VerticalOptions="Center" Text="{Binding typ}"  />                                                    </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Märke: "   />                                                    <Label VerticalOptions="Center" Text="{Binding prisPerDag}"  />                                                    </StackLayout>                                                <StackLayout Orientation="Horizontal">                                                   <Label VerticalOptions="Center" Text="Pris/Dag(kr): "   />                                                    <Label VerticalOptions="Center" Text="{Binding prisPerDag}" />                                                    </StackLayout>                                               </StackLayout>                                                    <Image HorizontalOptions="EndAndExpand" Source="RemoveImage.png"                                                           WidthRequest="50" HeightRequest="50">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer                                                                Tapped="datorListSelected_ItemTapped_Remove"                                                                CommandParameter="{Binding datorId}">                                                                                                                            </TapGestureRecognizer>                                                        </Image.GestureRecognizers>                                                    </Image>                                                        </StackLayout>                                                    </Frame>                                            </Grid>                                        </ViewCell>                                    </DataTemplate>                                </ListView.ItemTemplate>                            </ListView>                                 </Frame>                             <StackLayout Orientation="Horizontal" HorizontalOptions="EndAndExpand">                <Button Text="Go Back" Clicked="Button_Go_Back"></Button>                <Button Text="Submit" Clicked="Button_ADD_Selected"></Button>                                 </StackLayout>            </StackLayout>                    </StackLayout>                </StackLayout>            </StackLayout>                   </StackLayout>    </ContentPage.Content></ContentPage>